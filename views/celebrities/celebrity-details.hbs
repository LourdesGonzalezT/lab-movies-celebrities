<h2>CELEBRITY DETAILS</h2>

<div>
    <article>
        <h5>NAME: {{name}}</h5>
        <h5>OCCUPATION: {{occupation}}</h5>
        <h5>CATCH PHRASE: {{catchPhrase}}</h5>
    </article>
    <div class="btn edit">
        <a href="/celebrities/{{_id}}/edit" class="btn btn-dark">Edit celebrity</a>
    </div>
    <div class="btn delete">
        <form action="/celebrities/{{_id}}/delete" method="POST">
            <input type="submit" value="Delete celebrity" class="btn btn-dark">
        </form>
    </div>
</div>