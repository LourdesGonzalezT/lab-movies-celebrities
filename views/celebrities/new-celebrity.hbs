<h2>NEW CELEBRITY</h2>

<form action="/celebrities/create" method="POST">
    <div class="mb-3">
        <label for="name" class="form-label">Name</label>
        <input type="text" class="form-control" id="celebrityName" name="name">
    </div>
    <div class="mb-3">
        <label for="occupation" class="form-label">Occupation</label>
        <input type="text" class="form-control" id="celebrityOccupation" name="occupation">
    </div>
    <div class="mb-3">
        <label for="catchPhrase" class="form-label">Phrase</label>
        <input type="text" class="form-control" id="celebrituyCatchPhrase" name="catchPhrase">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>